  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "args": [
        "index.ts"
      ],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "./node_modules/ts-node/register"
      ],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}/backend",
      "env": {
        "NODE_ENV": "develop"
      },
      "protocol": "inspector",
    }
  ]
}